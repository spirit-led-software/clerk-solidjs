{
  "name": "clerk-solidjs",
  "description": "Clerk for SolidJS",
  "private": false,
  "version": "1.1.17",
  "keywords": [
    "solidjs",
    "clerk",
    "solid",
    "clerk-auth"
  ],
  "bugs": {
    "url": "https://github.com/spirit-led-software/clerk-solidjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spirit-led-software/clerk-solidjs"
  },
  "license": "MIT",
  "author": "Spirit-Led Software",
  "type": "module",
  "main": "./dist/index/server.cjs",
  "module": "./dist/index/server.js",
  "types": "./dist/index/index.d.ts",
  "typesVersions": {
    "*": {
      "errors": [
        "./dist/errors/index.d.ts"
      ],
      "server": [
        "./dist/server/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "worker": {
        "solid": "./dist/index/server.jsx",
        "import": {
          "types": "./dist/index/index.d.ts",
          "default": "./dist/index/server.js"
        },
        "require": {
          "types": "./dist/index/index.d.cts",
          "default": "./dist/index/server.cjs"
        }
      },
      "browser": {
        "solid": "./dist/index/index.jsx",
        "import": {
          "types": "./dist/index/index.d.ts",
          "default": "./dist/index/index.js"
        },
        "require": {
          "types": "./dist/index/index.d.cts",
          "default": "./dist/index/index.cjs"
        }
      },
      "deno": {
        "solid": "./dist/index/server.jsx",
        "import": {
          "types": "./dist/index/index.d.ts",
          "default": "./dist/index/server.js"
        },
        "require": {
          "types": "./dist/index/index.d.cts",
          "default": "./dist/index/server.cjs"
        }
      },
      "node": {
        "solid": "./dist/index/server.jsx",
        "import": {
          "types": "./dist/index/index.d.ts",
          "default": "./dist/index/server.js"
        },
        "require": {
          "types": "./dist/index/index.d.cts",
          "default": "./dist/index/server.cjs"
        }
      },
      "solid": "./dist/index/index.jsx",
      "import": {
        "types": "./dist/index/index.d.ts",
        "default": "./dist/index/index.js"
      },
      "require": {
        "types": "./dist/index/index.d.cts",
        "default": "./dist/index/index.cjs"
      }
    },
    "./errors": {
      "import": {
        "types": "./dist/errors/index.d.ts",
        "default": "./dist/errors/index.js"
      },
      "require": {
        "types": "./dist/errors/index.d.cts",
        "default": "./dist/errors/index.cjs"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/server/index.d.ts",
        "default": "./dist/server/index.js"
      },
      "require": {
        "types": "./dist/server/index.d.cts",
        "default": "./dist/server/index.cjs"
      }
    }
  },
  "browser": {
    "./dist/index/server.js": "./dist/index/index.js",
    "./dist/index/server.cjs": "./dist/index/index.cjs"
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch .",
    "dev:publish": "pnpm run dev -- --env.publish",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "clean": "rimraf dist",
    "publish:local": "pnpm dlx yalc push --replace  --sig"
  },
  "dependencies": {
    "@clerk/backend": "^1.8.1",
    "@clerk/shared": "^2.5.3",
    "@solid-primitives/context": "^0.2.3",
    "@solid-primitives/destructure": "^0.1.17",
    "@solid-primitives/memo": "^1.3.9",
    "@tanstack/solid-query": "^5.52.2"
  },
  "devDependencies": {
    "@clerk/clerk-js": "^5.20.0",
    "@clerk/localizations": "^2.8.1",
    "@clerk/themes": "^2.1.27",
    "@clerk/types": "^4.19.0",
    "@eslint/js": "^9.10.0",
    "@solidjs/testing-library": "^0.8.9",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.5.4",
    "eslint": "^9.10.0",
    "expect-type": "^0.20.0",
    "globals": "^15.9.0",
    "jsdom": "^25.0.0",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "^4.0.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.2.4",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0",
    "vite-plugin-solid": "^2.10.2",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
  },
  "peerDependencies": {
    "@solidjs/start": ">=1",
    "solid-js": ">=1"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
